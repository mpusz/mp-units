    Priority: 1
  - Regex: '^".+/.*'
    Priority: 2
  - Regex: "^<.+/.*"
    Priority: 3
  - Regex: '^<.*\.h>'
    Priority: 4
  - Regex: "^<.*"
    Priority: 5
